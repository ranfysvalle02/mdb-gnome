{
  "slug": "data_imaging",
  "name": "Workout Analyzer",
  "status": "active",
  "auth_required": false,
  "data_scope": ["self"],
  "managed_indexes": {
    "workouts": [
      {
        "name": "workout_vector_index",
        "type": "vectorSearch",
        "definition": {
          "fields": [
            {
              "type": "vector",
              "path": "workout_vector",
              "numDimensions": 192,
              "similarity": "cosine"
            },
            {
              "type": "filter",
              "path": "experiment_id"
            },
            {
              "type": "filter",
              "path": "_id"
            }
          ]
        }
      },
      {
        "name": "id_regular_index",
        "type": "regular",
        "keys": { "_id": 1 }
      }
    ]
  }
}